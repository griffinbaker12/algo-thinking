#include "common.h"

#define SIZE 100000
#define SNOWFLAKE_LEN 6

int code(int snowflake[]) {
  int sum = 0, i;
  for (i = 0; i < SNOWFLAKE_LEN; i++) {
    sum += snowflake[i]
  }
}

int identify_identical(int values[], int len) {
  int i, j;
  for (i = 0; i < len; i++) {
    for (j = i + 1; j < len; j++) {
      if (values[i] == values[j]) {
        return 1;
      }
    }
  }
  return 0;
}

void identify_right(int snow1[], int snow2[], int start) {
  for (int i = 0; i < SNOWFLAKE_LEN; i++) {
    if (snow1[i] == snow2[(i + start) % SNOWFLAKE_LEN]) {
      printf("Identical values found: %d\n", snow1[i]);
    }
  }
}

int main(void) {
  int a[5] = {1, 2, 3, 1, 5};
  identify_identical(a, 5);
  return 0;
}
